[
// Fold all code blocks
  { "keys": ["alt+0", "alt+0"],
    "command": "syntaxfold_fold_all" },

// Unfold all code blocks
  { "keys": ["alt+shift+0", "alt+shift+0"],
    "command": "syntaxfold_unfold_all"},

// Toggle fold current code block
  { "keys": ["alt+1", "alt+1"],
    "command": "syntaxfold_toggle_fold_current"},
]
